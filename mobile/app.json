{
  "expo": {
    "name": "Proffy",
    "slug": "Proffy",
    "privacy": "public",
    "sdkVersion": "38.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/icon.png",
    "splash": {
      "image": "./assets/img/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#9871f5"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.leoronne.proffy"
    },
    "android": {
      "package": "com.leoronne.proffy",
      "icon": "./assets/img/icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/logoTransparent.png",
        "backgroundColor": "#9871f5"
      }
    }
  }
}
