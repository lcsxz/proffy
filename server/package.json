{
  "name": "proffy-server",
  "repository": "https://github.com/leoronne/proffy.git",
  "author": "Leonardo Ronne",
  "license": "MIT",
  "version": "1.0.0",
  "main": "server.ts",
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules src/server.ts",
    "start": "node -r ./tsconfig-paths-bootstrap.js dist/server.js",
    "knex:migrate": "knex --knexfile dist/knexfile.js migrate:latest",
    "knex:migrate:undo": "knex --knexfile dist/knexfile.js migrate:rollback",
    "knex:seed": "knex --knexfile dist/knexfile.js seed:run",
    "build": "tsc"
  },
  "dependencies": {
    "celebrate": "^12.2.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "knex": "^0.21.2",
    "pg": "^8.3.0",
    "tsconfig-paths": "^3.9.0",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/node": "^14.0.27",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.4",
    "nodemon": "^2.0.2",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.9.7"
  }
}
